<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
</head>
<body>
    <h1>Restaurant Application List</h1>
    <% if(restaurants.length > 0 ) { %> 
        <table width="400" border="0">
            <tr>
                <td align="center">No.</td>
                <td align="center">Restaurant Name</td>
                <td align="center">Writer</td>
            </tr>
    <%      restaurants.forEach(function(item, index) {%>
            <tr>
                <td align="center"> <%=index+1%></td>
                <td align="center"> <a href="/form/permit/<%=restaurants[index].Application_Code%>"><%=restaurants[index].Restaurant_Name%></a></td>
                <td align="center"> <%=restaurants[index].Manager_Code%></td>
            </tr>            
    <%      })%>
        </table>
    <% } else { %>
        <p>No Application </p>
    <% } %>
    <input type="button" value="Back" onclick="history.back();">
</body>
</html>